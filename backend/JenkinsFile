pipeline{
   agent any
    tools{
            nodejs 'node20'
    }
   stages{
       stage("Checkout"){
           steps{
               checkout scm
           }
       }
       stage("Test"){
           steps{
               sh 'npm install'
           }
    }
       stage("Build"){
           steps{
               sh 'npm run build'
           }
       }
   } 
}
